<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LanceRequete</servlet-name>
        <servlet-class>LanceRequete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LanceRequete</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Correcteur</servlet-name>
        <servlet-class>Correcteur</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Correcteur</servlet-name>
        <url-pattern>/correcteur</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Modifier</servlet-name>
        <servlet-class>Modifier</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Modifier</servlet-name>
        <url-pattern>/modifier</url-pattern>
    </servlet-mapping>

</web-app>